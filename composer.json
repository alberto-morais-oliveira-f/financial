{
    "name": "am2tec/financial",
    "description": "Módulo Financeiro Genérico (General Ledger) para Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto",
            "email": "albertomorais80@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "spatie/laravel-data": "^4.0",
        "dragonmantank/cron-expression": "^3.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Am2tec\\Financial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Am2tec\\Financial\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Am2tec\\Financial\\Infrastructure\\Providers\\FinancialServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}